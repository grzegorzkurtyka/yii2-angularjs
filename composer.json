{
  "name": "tesjin/yii2-angularjs",
  "type": "library",
  "description": "The assets bundle of Angular.js for Yii2. Managed by bower",
  "keywords": ["angular", "angularjs", "angular.js", "yii", "yii2"],
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Vladislav Orlov",
      "email": "orlov@tesjin.ru",
      "homepage": "http://www.tesjin.ru",
      "role": "Developer"
    },
    {
      "name": "Grzegorz Kurtyka",
      "email": "grzegorz.kurtyka@schibsted.pl",
      "homepage": "http://schibsted.pl",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "support@tesjin.ru",
    "issues": "https://github.com/grzegorzkurtyka/yii2-angularjs/issues",
    "source": "https://github.com/grzegorzkurtyka/yii2-angularjs"
  },
  "require": {
    "php": ">=5.3.0",
    "bower-asset/angular": "~1.0"
  },
  "autoload": {
    "psr-4": {
      "yii\\angularjs\\": ""
    }
  },
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  }
}
