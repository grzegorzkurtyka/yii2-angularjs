  "description": "The assets bundle of Angular.js for Yii2. Managed by bower",
  "keywords": ["angular", "angularjs", "angular.js", "yii", "yii2"],
  "license": "BSD-3-Clause",
  "version": "1.3.15",
  "authors": [
    {
      "name": "Vladislav Orlov",
      "email": "orlov@tesjin.ru",
      "homepage": "http://www.tesjin.ru",
      "role": "Developer"
    },
    {
      "name": "Grzegorz Kurtyka",
      "email": "grzegorz.kurtyka@schibsted.pl",
      "homepage": "http://schibsted.pl",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.3.0",
    "bower-asset/angular": "1.3.15",
    "bower-asset/angular-resource": "1.3.15",
    "bower-asset/angular-ui-router": "~0.2"
  },
  "autoload": {
    "psr-4": {
      "yii\\angularjs\\": ""
